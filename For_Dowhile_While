public class App {
    public static void main(String[] args) throws Exception {

        for (int i = 10; i != 0; i--) {
            System.out.println("spanish bread");
        }
        
        int rideCount = 5;
        do {
            System.out.println("Dinasor");
            rideCount--;
        } while (rideCount != 0);

            int nickname = 5;

        while (nickname != 0) {
            System.out.println("Dragon");
            nickname--;
        }
    
        
    }

}
